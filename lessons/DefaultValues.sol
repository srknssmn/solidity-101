pragma solidity ^0.8.7;

contract DefaultValues{
    bool public b;          // default value is false
    uint public ui;         // default value is 0
    int public i;           // default value is 0
    address public add;     // default value is 0x0000..
    bytes32 public byt;     // default value is 0x00000000000..
}